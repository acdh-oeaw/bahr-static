   <text type="diaryDay">
      <body>
         <div type="writingSession" n="1">
            <p>
               <date when="1904-12-09" n="01">9.</date> In der Probe von <rs type="person" ref="#pmb12163">Mahlers</rs>
               <space unit="chars" quantity="1"/>
               <rs type="work" ref="#pmb30123">3. Symphonie</rs>.</p>
            <p>
               <rs type="work" subtype="implied" ref="#pmb31177">Dictiert</rs> über <rs type="person" ref="#pmb2121">Schnitzlers</rs>
               <space unit="chars" quantity="1"/>
               <rs type="work" ref="#pmb29873">Puppenspieler</rs>.</p>
            <p>
               <subst>
                  <del rend="overwritten">
                     <gap quantity="1" reason="illegible" unit="chars"/>
                  </del>
                  <add place="inline">B</add>
               </subst>ei <rs type="person" ref="#pmb27882">Wärndorfer</rs>, <rs type="person" ref="#pmb11941">Klimts</rs>
               <space unit="chars" quantity="1"/>
               <rs type="work" ref="#pmb31167">Hoffnung</rs> und dann die <rs type="person" ref="#pmb13870">Beardsleys</rs> angesehen. Mit <rs type="person" ref="#pmb11941">Klimt</rs>, <rs type="person" ref="#pmb3280">Kolo Moser</rs> und <rs type="person" ref="#pmb11730">Hoffmann</rs>.</p>
         </div>
      </body>
