   <text type="card">
      <body>
         <div type="address">
            <address>
               <addrLine>Herrn D<hi rend="superscript">r</hi> Arthur Schnitzler</addrLine>
               <addrLine>
                  <rs type="place" ref="#pmb51">Wien I</rs>
               </addrLine>
               <addrLine>
                  <rs type="place" ref="#pmb397">Frankgasse 3</rs>
               </addrLine>
            </address>
         </div>
         <div type="writingSession" n="1">
            <opener>
               <salute>Lieber Arthur!</salute>
            </opener>
            <p>Ich kann leider <anchor type="commentary" xml:id="KL167-1">morgen</anchor> früh absolut nicht, komme <hi rend="underline" n="1">vielleicht</hi> Nachmittag nach.</p>
            <closer>Herzlichst<lb/>
               <signed>Hermann</signed>
            </closer>
         </div>
      </body>
